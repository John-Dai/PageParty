<h1>Listing code_lines</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_lines.each do |code_line| %>
      <tr>
        <td><%= code_line.code %></td>
        <td><%= link_to 'Show', code_line %></td>
        <td><%= link_to 'Edit', edit_code_line_path(code_line) %></td>
        <td><%= link_to 'Destroy', code_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code line', new_code_line_path %>
