<h1>Listing vote_lines</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Votes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_lines.each do |vote_line| %>
      <tr>
        <td><%= vote_line.code %></td>
        <td><%= vote_line.votes %></td>
        <td><%= link_to 'Show', vote_line %></td>
        <td><%= link_to 'Edit', edit_vote_line_path(vote_line) %></td>
        <td><%= link_to 'Destroy', vote_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote line', new_vote_line_path %>
